<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
	<sitecore>
		<javaScriptServices>
			<apps>
				<app name="defaults">
					<!-- This allows us to use a layout other than the JSS Layout which is protected -->
					<patch:attribute name="layoutTemplate">/sitecore/layout/Layouts/Project/demo-site/Demo Layout</patch:attribute>
				</app>
			</apps>
		</javaScriptServices>
	</sitecore>
</configuration>